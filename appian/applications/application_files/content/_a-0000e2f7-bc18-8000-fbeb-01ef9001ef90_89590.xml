    <versionUuid>_a-0000e2f7-bc18-8000-fbeb-01ef9001ef90_89657</versionUuid>
    <rule>
        <name>VFM_isVehicleDeniedTest</name>
        <uuid>_a-0000e2f7-bc18-8000-fbeb-01ef9001ef90_89590</uuid>
        <description>Based on the response from the supervisor and the number of count of previous edits, determines whether the insert of a new vehicle should be approved or denied.</description>
        <parentUuid>_a-0000e2f7-bc18-8000-fbeb-01ef9001ef90_89558</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    ri!counter &gt; #"_a-0000e2f7-bc18-8000-fbeb-01ef9001ef90_89585",
    ri!decision = "Reject"
  ),
  "Vehicle is denied",
  "Vehicle can be Accepted"
)</definition>
        <namedTypedValue>
            <name>counter</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>decision</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
