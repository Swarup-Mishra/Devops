    <versionUuid>_a-0000e2f7-bc18-8000-fbeb-01ef9001ef90_89726</versionUuid>
    <rule>
        <name>VFM_calculateNextServicingDate</name>
        <uuid>_a-0000e2f7-bc18-8000-fbeb-01ef9001ef90_89658</uuid>
        <description>When given the date updated and the category of vehicle, calculates the next service date of the vehicle.</description>
        <parentUuid>_a-0000e2f7-bc18-8000-fbeb-01ef9001ef90_89558</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  ri!vehicleType = "Luxury",
  edate(ri!dateUpdated,3),
  edate(ri!dateUpdated,6)
)</definition>
        <namedTypedValue>
            <name>vehicleType</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>dateUpdated</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
